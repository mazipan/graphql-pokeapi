{
  "name": "graphql-pokeapi",
  "description": "GraphQL implementation of pokeapi.co",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./src/index.ts",
  "scripts": {
    "clean:dist": "rimraf dist",
    "test": "jest",
    "lint": "standard --fix",
    "build": "babel ./src --out-dir ./dist",
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "postinstall": "yarn clean:dist && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "jest": "25.1.0",
    "rimraf": "3.0.2",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "apollo-cache-control": "0.8.11",
    "apollo-server": "2.14.2",
    "apollo-server-express": "2.10.0",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "graphql-tag": "2.10.3",
    "graphql-type-json": "0.3.1",
    "graphql-upload": "10.0.0",
    "node-fetch": "2.6.0",
    "nodemon": "^2.0.2",
    "pokedex-promise-v2": "^3.2.0"
  },
  "engines": {
    "node": ">= 10.x"
  }
}
